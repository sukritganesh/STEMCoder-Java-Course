
public class IntroToVariables {

	public static void main(String[] args) {
	
		int x = 5;
		double y = 8.7;
		double longNum = 700000000000000000000000.0;
		double num = 8;
		String hello = "hello";
		char myCharacter = 'h';
		char numChar = '7';
		double tooLong = 88888888888888888888888888888888888;
		double tooPrecise = 373.47181281281827236127638261;
		

	}

}
